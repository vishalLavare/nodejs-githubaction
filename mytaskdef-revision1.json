{
  "family": "mytask-family",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",  # Required for Fargate
  "cpu": "256",  # Set the appropriate CPU for your container
  "memory": "512",  # Set the appropriate memory for your container
  "containerDefinitions": [
    {
      "name": "mynodecont",
      "image": "your-ecr-repo:tag",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]
}
